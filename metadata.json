{
  "name": "simp-selinux",
  "version": "2.6.2",
  "author": "SIMP Team",
  "summary": "manages the SELinux system state",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-selinux",
  "project_page": "https://github.com/simp/pupmod-simp-selinux",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "selinux",
    "setenforce"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.9.0 < 7.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.1.0 < 5.0.0"
    },
    {
      "name": "simp/vox_selinux",
      "version_requirement": ">= 3.0.0 < 4.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "camptocamp/systemd",
        "version_requirement": ">= 2.2.0 < 3.0.0"
      }
    ]
  },
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.0.0 < 7.0.0"
    }
  ]
}
